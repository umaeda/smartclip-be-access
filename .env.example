# API Configuration
API_STR=/api
PROJECT_NAME=FastAPI Backend

# Security
# Gere uma chave secreta segura com: openssl rand -hex 32
SECRET_KEY=
ACCESS_TOKEN_EXPIRE_MINUTES=11520

# CORS
BACKEND_CORS_ORIGINS=http://localhost:3000,http://localhost:8080,http://localhost:4200

# Database
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=
POSTGRES_DB=app_db
# Você pode definir a URI completa ou deixar que seja construída a partir dos valores acima
# SQLALCHEMY_DATABASE_URI=postgresql://user:password@server/db
# DATABASE_URL=postgresql://user:password@server/db

# Google OAuth
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:8000/api/auth/google/callback

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60